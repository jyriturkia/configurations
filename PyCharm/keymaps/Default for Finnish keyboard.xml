<keymap version="1" name="Default for Finnish keyboard" parent="$default">
  <action id="CollapseAll">
    <keyboard-shortcut first-keystroke="ctrl subtract" />
    <keyboard-shortcut first-keystroke="ctrl plus" />
  </action>
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl subtract" />
    <keyboard-shortcut first-keystroke="shift ctrl plus" />
  </action>
  <action id="CollapseExpandableComponent">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl subtract" />
    <keyboard-shortcut first-keystroke="ctrl plus" />
  </action>
  <action id="CollapseRegion">
    <keyboard-shortcut first-keystroke="ctrl subtract" />
    <keyboard-shortcut first-keystroke="ctrl plus" />
  </action>
  <action id="CollapseRegionRecursively">
    <keyboard-shortcut first-keystroke="ctrl alt subtract" />
    <keyboard-shortcut first-keystroke="ctrl alt plus" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl minus" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
  </action>
  <action id="EditorCodeBlockEnd">
    <keyboard-shortcut first-keystroke="ctrl dead_diaeresis" />
  </action>
  <action id="EditorCodeBlockEndWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl dead_circumflex" />
  </action>
  <action id="EditorCodeBlockStart">
    <keyboard-shortcut first-keystroke="ctrl #10000c5" />
  </action>
  <action id="EditorCodeBlockStartWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl #10000c5" />
  </action>
  <action id="EmmetNextEditPoint">
    <keyboard-shortcut first-keystroke="shift alt dead_circumflex" />
  </action>
  <action id="EmmetPreviousEditPoint">
    <keyboard-shortcut first-keystroke="shift alt #10000c5" />
  </action>
  <action id="ExpandAll">
    <keyboard-shortcut first-keystroke="ctrl add" />
    <keyboard-shortcut first-keystroke="ctrl dead_acute" />
  </action>
  <action id="ExpandAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl add" />
    <keyboard-shortcut first-keystroke="shift ctrl dead_acute" />
  </action>
  <action id="ExpandExpandableComponent">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl add" />
    <keyboard-shortcut first-keystroke="ctrl dead_acute" />
  </action>
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="ctrl add" />
    <keyboard-shortcut first-keystroke="ctrl dead_acute" />
  </action>
  <action id="ExpandRegionRecursively">
    <keyboard-shortcut first-keystroke="ctrl alt add" />
    <keyboard-shortcut first-keystroke="ctrl alt dead_acute" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="shift ctrl #10000a7" />
  </action>
  <action id="HippieBackwardCompletion">
    <keyboard-shortcut first-keystroke="shift alt minus" />
  </action>
  <action id="HippieCompletion">
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="Images.Editor.ActualSize">
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl minus" />
  </action>
  <action id="MaintenanceAction">
    <keyboard-shortcut first-keystroke="shift ctrl alt minus" />
  </action>
  <action id="MaximizeToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl #10000c4" />
  </action>
  <action id="QuickChangeScheme">
    <keyboard-shortcut first-keystroke="ctrl #10000a7" />
  </action>
  <action id="Vcs.QuickListPopupAction">
    <keyboard-shortcut first-keystroke="ctrl alt #10000a7" />
  </action>
</keymap>